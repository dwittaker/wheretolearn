<h1>Listing subject_modules</h1>

<table>
  <tr>
    
    
    <th>Subjectid</th>
    <th>Name</th>
    <th>Description</th>
    <th>Purpose</th>
    <th>Order</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @subject_modules.each do |subject_module| %>
  <tr>
    
    
    <td><%= subject_module.subject_id %></td>
    <td><%= subject_module.smname %></td>
    <td><%= subject_module.smdescription %></td>
    <td><%= subject_module.smpurpose %></td>
    <td><%= subject_module.order %></td>
    <td><%= link_to 'Show', subject_module %></td>
    <td><%= link_to 'Edit', edit_subject_module_path(subject_module) %></td>
    <td><%= link_to 'Destroy', subject_module, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

